expression: result
---
SELECT
  [rows].[row_json] AS [rows]
FROM
  (
    SELECT
      [albums].[albums] AS [albums],
      [Albums].[Albums] AS [Albums]
    FROM
      [public].[Artist] AS [Artist]
      OUTER APPLY (
        SELECT
          [rows].[json] AS [rows]
        FROM
          (
            SELECT
              [Album].[Title] AS [title]
            FROM
              [public].[Album] AS [Album]
            WHERE
              (
                (1 = 1)
                AND ([Artist].[ArtistId] = [Album].[ArtistId])
              ) FOR JSON PATH,
              INCLUDE_NULL_VALUES
          ) AS [rows]([json]) FOR JSON PATH,
          INCLUDE_NULL_VALUES,
          WITHOUT_ARRAY_WRAPPER
      ) AS [albums]([json])
      OUTER APPLY (
        SELECT
          [rows].[json] AS [rows]
        FROM
          (
            SELECT
              [Album].[Title] AS [title]
            FROM
              [public].[Album] AS [Album]
            WHERE
              (
                (1 = 1)
                AND ([Artist].[ArtistId] = [Album].[ArtistId])
              ) FOR JSON PATH,
              INCLUDE_NULL_VALUES
          ) AS [rows]([json]) FOR JSON PATH,
          INCLUDE_NULL_VALUES,
          WITHOUT_ARRAY_WRAPPER
      ) AS [Albums]([json])
    ORDER BY
      [Artist].[ArtistId] ASC OFFSET 0 ROWS FETCH NEXT 5 ROWS ONLY FOR JSON PATH,
      INCLUDE_NULL_VALUES
  ) AS [rows]([row_json]) FOR JSON PATH,
  INCLUDE_NULL_VALUES,
  WITHOUT_ARRAY_WRAPPER


SELECT
  [rows].[json] AS [rows]
FROM
  (
    SELECT
      [Artist].[Artist] AS [Artist]
    FROM
      [public].[Album] AS [Album]
      LEFT OUTER JOIN LATERAL (
        SELECT
          [rows].[rows] AS [rows]
        FROM
          (
            SELECT
              [Artist].[Name] AS [Name]
            FROM
              [public].[Artist] AS [Artist]
            WHERE
              (
                (1 = 1)
                AND ([Album].[ArtistId] = [Artist].[ArtistId])
              ) FOR JSON PATH
          ) AS [rows]([json]) FOR JSON PATH,
          WITHOUT_ARRAY_WRAPPER
      ) AS [Artist] ON ('true')
    ORDER BY
      [Album].[AlbumId] ASC OFFSET 0 ROWS FETCH NEXT 5 ROWS ONLY FOR JSON PATH
  ) AS [rows]([json]) FOR JSON PATH,
  WITHOUT_ARRAY_WRAPPER

[]

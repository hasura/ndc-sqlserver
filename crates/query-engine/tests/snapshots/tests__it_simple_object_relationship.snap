---
source: crates/query-engine/tests/tests.rs
expression: result
---
SELECT
  [rows].[row_json] AS [rows]
FROM
  (
    SELECT
      [Artist].[Artist] AS [Artist]
    FROM
      [public].[Album] AS [Album]
      OUTER APPLY (
        SELECT
          [rows].[json] AS [rows]
        FROM
          (
            SELECT
              [Artist].[Name] AS [Name]
            FROM
              [public].[Artist] AS [Artist]
            WHERE
              (
                (1 = 1)
                AND ([Album].[ArtistId] = [Artist].[ArtistId])
              ) FOR JSON PATH,
              INCLUDE_NULL_VALUES
          ) AS [rows]([json]) FOR JSON PATH,
          INCLUDE_NULL_VALUES,
          WITHOUT_ARRAY_WRAPPER
      ) AS [Artist]([json])
    ORDER BY
      [Album].[AlbumId] ASC OFFSET 0 ROWS FETCH NEXT 5 ROWS ONLY FOR JSON PATH,
      INCLUDE_NULL_VALUES
  ) AS [rows]([row_json]) FOR JSON PATH,
  INCLUDE_NULL_VALUES,
  WITHOUT_ARRAY_WRAPPER

[]

SELECT
  [rows].[json] AS [rows]
FROM
  (
    SELECT
      [Albums].[Albums] AS [Albums]
    FROM
      [public].[Artist] AS [Artist]
      OUTER APPLY (
        SELECT
          [rows].[json] AS [rows]
        FROM
          (
            SELECT
              [Tracks].[Tracks] AS [Tracks]
            FROM
              [public].[Album] AS [Album]
              OUTER APPLY (
                SELECT
                  [rows].[json] AS [rows]
                FROM
                  (
                    SELECT
                      [Track].[Name] AS [name]
                    FROM
                      [public].[Track] AS [Track]
                    WHERE
                      (
                        (1 = 1)
                        AND ([Album].[AlbumId] = [Track].[AlbumId])
                      ) FOR JSON PATH,
                      INCLUDE_NULL_VALUES
                  ) AS [rows]([json]) FOR JSON PATH,
                  INCLUDE_NULL_VALUES,
                  WITHOUT_ARRAY_WRAPPER
              ) AS [Tracks]([json])
            WHERE
              (
                (1 = 1)
                AND ([Artist].[ArtistId] = [Album].[ArtistId])
              ) FOR JSON PATH,
              INCLUDE_NULL_VALUES
          ) AS [rows]([json]) FOR JSON PATH,
          INCLUDE_NULL_VALUES,
          WITHOUT_ARRAY_WRAPPER
      ) AS [Albums]([json]) FOR JSON PATH,
      INCLUDE_NULL_VALUES
  ) AS [rows]([json]) FOR JSON PATH,
  INCLUDE_NULL_VALUES,
  WITHOUT_ARRAY_WRAPPER

[]

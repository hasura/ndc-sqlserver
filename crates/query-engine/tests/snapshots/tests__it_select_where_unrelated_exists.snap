SELECT
  [rows].[json] AS [rows]
FROM
  (
    SELECT
      [album].[Title] AS [title]
    FROM
      [public].[Album] AS [album]
    WHERE
      EXISTS (
        SELECT
        FROM
          [public].[Artist] AS [artist]
        WHERE
          (
            (
              (1 = 1)
              AND ([artist].[Name] = @P1)
            )
            AND ([album].[ArtistId] = [artist].[ArtistId])
          ) FOR JSON PATH
      ) FOR JSON PATH
  ) AS [rows]([json]) FOR JSON PATH,
  WITHOUT_ARRAY_WRAPPER

[(1, String("Gilberto Gil"))]

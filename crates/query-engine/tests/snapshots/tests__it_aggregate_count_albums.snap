---
source: crates/query-engine/tests/tests.rs
expression: result
---
SELECT
  JSON_QUERY([rows].[row_json], '$.row_json') AS [rows],
  JSON_QUERY(
    JSON_VALUE([aggregates].[agg_json], '$.agg_json'),
    '$'
  ) AS [aggregates]
FROM
  (
    SELECT
      *
    FROM
      (
        SELECT
          [Album].[Title] AS [Title]
        FROM
          [public].[Album] AS [Album]
        ORDER BY
          [Album].[AlbumId] ASC OFFSET 3 ROWS FETCH NEXT 5 ROWS ONLY FOR JSON PATH,
          INCLUDE_NULL_VALUES
      ) AS [universe]([row_json]) FOR JSON PATH,
      INCLUDE_NULL_VALUES,
      WITHOUT_ARRAY_WRAPPER
  ) AS [rows]([row_json])
  CROSS JOIN (
    SELECT
      *
    FROM
      (
        SELECT
          COUNT(*) AS [how_many_albums]
        FROM
          [public].[Album] AS [Album] FOR JSON PATH,
          INCLUDE_NULL_VALUES,
          WITHOUT_ARRAY_WRAPPER
      ) AS [aggregates]([agg_json]) FOR JSON PATH,
      INCLUDE_NULL_VALUES,
      WITHOUT_ARRAY_WRAPPER
  ) AS [aggregates]([agg_json]) FOR JSON PATH,
  INCLUDE_NULL_VALUES,
  WITHOUT_ARRAY_WRAPPER

[]

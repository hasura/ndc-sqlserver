SELECT
  row_to_json([universe]) AS [universe]
FROM
  (
    SELECT
      *
    FROM
      (
        SELECT
          COUNT(DISTINCT [Album].[ArtistId]) AS [how_many_distinct_artist_ids]
        FROM
          [public].[Album] AS [Album] FOR JSON PATH
      ) AS [aggregates] FOR JSON PATH
  ) AS [universe] FOR JSON PATH


SELECT
  JSON_QUERY([aggregates].[json], '$') AS [aggregates]
FROM
  (
    SELECT
      COUNT(DISTINCT [Album].[ArtistId]) AS [how_many_distinct_artist_ids]
    FROM
      [public].[Album] AS [Album] FOR JSON PATH,
      INCLUDE_NULL_VALUES,
      WITHOUT_ARRAY_WRAPPER
  ) AS [aggregates]([json]) FOR JSON PATH,
  INCLUDE_NULL_VALUES,
  WITHOUT_ARRAY_WRAPPER

[]

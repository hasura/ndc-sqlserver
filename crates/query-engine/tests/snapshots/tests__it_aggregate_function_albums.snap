SELECT
  row_to_json([universe]) AS [universe]
FROM
  (
    SELECT
      *
    FROM
      (
        SELECT
          max([Album].[ArtistId]) AS [max_artist_id]
        FROM
          [public].[Album] AS [Album] FOR JSON PATH,
          INCLUDE_NULL_VALUES
      ) AS [aggregates] FOR JSON PATH,
      INCLUDE_NULL_VALUES
  ) AS [universe] FOR JSON PATH,
  INCLUDE_NULL_VALUES


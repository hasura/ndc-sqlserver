SELECT
  JSON_QUERY([aggregates].[agg_json], '$') AS [aggregates]
FROM
  (
    SELECT
      max([Album].[ArtistId]) AS [max_artist_id]
    FROM
      [public].[Album] AS [Album] FOR JSON PATH,
      INCLUDE_NULL_VALUES,
      WITHOUT_ARRAY_WRAPPER
  ) AS [aggregates]([agg_json]) FOR JSON PATH,
  INCLUDE_NULL_VALUES,
  WITHOUT_ARRAY_WRAPPER

